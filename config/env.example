# =============================================================================
# SWE-AGENT Configuration - Copy this to .env and fill in your credentials
# =============================================================================

# =============================================================================
# JIRA Configuration
# =============================================================================
# Your Atlassian JIRA instance URL
JIRA_URL=https://your-company.atlassian.net

# JIRA API Token (Generate from: https://id.atlassian.com/manage-profile/security/api-tokens)
JIRA_TOKEN=your_jira_api_token_here

# JIRA Project Key (e.g., PROJ, DEV, etc.)
JIRA_PROJECT_KEY=PROJ

# JIRA User Email (for API authentication)
JIRA_USER_EMAIL=your-email@company.com

# =============================================================================
# GitHub Configuration
# =============================================================================
# GitHub Personal Access Token (Generate from: https://github.com/settings/tokens)
# Required scopes: repo, workflow, write:packages, delete:packages
GH_TOKEN=ghp_your_github_personal_access_token

# GitHub Repository (format: owner/repository-name)
GH_REPO=your-username/your-repository

# GitHub Username
GH_USERNAME=your-username

# Default branch for PRs
GH_DEFAULT_BRANCH=main

# =============================================================================
# Google Cloud / Gemini Configuration
# =============================================================================
# Google Cloud Project ID
GCP_PROJECT=your-gcp-project-id

# Google Cloud Region (recommended: us-central1)
GCP_REGION=us-central1

# Google Cloud Service Account Key (JSON file path or content)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your-service-account-key.json

# Alternative: Google Cloud Service Account Key as JSON string
# GCP_SERVICE_ACCOUNT_JSON={"type":"service_account","project_id":"..."}

# =============================================================================
# SonarQube Configuration (Optional)
# =============================================================================
# SonarQube Server URL (if using SonarCloud, use https://sonarcloud.io)
SONAR_HOST_URL=https://sonarcloud.io

# SonarQube Authentication Token
SONAR_TOKEN=your_sonarqube_token

# SonarQube Organization (for SonarCloud)
SONAR_ORGANIZATION=your-organization

# SonarQube Project Key
SONAR_PROJECT_KEY=your-project-key

# =============================================================================
# Maven Configuration (Optional)
# =============================================================================
# Maven Home Directory (auto-detected if not specified)
# MAVEN_HOME=/usr/local/maven

# Maven Settings File Path (optional)
# MAVEN_SETTINGS=/path/to/settings.xml

# =============================================================================
# Application Configuration
# =============================================================================
# Flask Environment
FLASK_ENV=development
FLASK_DEBUG=true

# Flask Secret Key (generate with: python -c "import secrets; print(secrets.token_hex(16))")
FLASK_SECRET_KEY=your_flask_secret_key_here

# API Server Configuration
API_HOST=0.0.0.0
API_PORT=5050

# Frontend Configuration
FRONTEND_PORT=5173
FRONTEND_HOST=localhost

# =============================================================================
# Workflow Configuration
# =============================================================================
# Maximum number of parallel workers for issue processing
MAX_WORKERS=3

# Maximum number of auto-fix rounds per issue
MAX_FIX_ROUNDS=3

# Default assignee for JIRA issues to process
DEFAULT_ASSIGNEE=AI-Agent

# Polling interval for frontend (milliseconds)
POLLING_INTERVAL=3000

# =============================================================================
# Logging Configuration
# =============================================================================
# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Log file path (optional, logs to console if not specified)
LOG_FILE=/tmp/swe-agent.log

# =============================================================================
# Development/Testing Configuration
# =============================================================================
# Enable mock responses for development (true/false)
ENABLE_MOCK_MODE=false

# Test repository path for local development
TEST_REPO_PATH=/tmp/test-repo

# =============================================================================
# Security Configuration
# =============================================================================
# Allowed origins for CORS (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Enable/disable authentication (for future implementation)
ENABLE_AUTH=false

# JWT Secret Key (for future authentication implementation)
JWT_SECRET_KEY=your_jwt_secret_key_here
